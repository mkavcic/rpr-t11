<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="259.0" prefWidth="478.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="15.0" layoutY="30.0" prefHeight="200.0" prefWidth="448.0">
         <children>
            <MenuBar>
              <menus>
                <Menu id="fileBtn" fx:id="file" mnemonicParsing="false" text="%file">
                  <items>
                    <MenuItem mnemonicParsing="false" text="%close" />
                        <MenuItem id="printBtn" fx:id="printBtn" mnemonicParsing="false" onAction="#stampajGradove" text="%print" />
                  </items>
                </Menu>
                <Menu fx:id="edit" mnemonicParsing="false" text="%edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="%delete" />
                  </items>
                </Menu>
                <Menu fx:id="help" mnemonicParsing="false" text="%help">
                  <items>
                    <MenuItem fx:id="about" mnemonicParsing="false" text="%about" />
                  </items>
                </Menu>
                  <Menu fx:id="view" mnemonicParsing="false" text="%view">
                    <items>
                        <Menu fx:id="jezik" mnemonicParsing="false" text="%language">
                          <items>
                              <MenuItem fx:id="english" mnemonicParsing="false" text="%english"  onAction="#promijeni"/>
                              <MenuItem fx:id="german" mnemonicParsing="false" text="%german" onAction="#promijeni" />
                            <MenuItem fx:id="french" mnemonicParsing="false" text="%french" onAction="#promijeni" />
                              <MenuItem fx:id="bosnian" mnemonicParsing="false" text="%bosnian" onAction="#promijeni" />
                          </items>
                        </Menu>
                    </items>
                  </Menu>
              </menus>
            </MenuBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
